{
    "categories": [
        {
            "title": "## 💻 Frontend",
            "labels": [
                "frontend",
                "javascript"
            ]
        },
        {
            "title": "## 🔧 Backend",
            "labels": [
                "api",
                "backend",
                "index",
                "indexes",
                "router",
                "schema",
                "sender"
            ]
        },
        {
            "title": "## 🚀 Pipelines",
            "labels": [
                "automated",
                "checks",
                "deployment",
                "DevOps",
                "pipeline",
                "scan",
                "workflow"
            ]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": [
                "bug",
                "correct",
                "fix",
                "fixed",
                "fixes",
                "fixing"
            ]
        },
        {
            "title": "## 🧪 Tests",
            "labels": [
                "test",
                "tests"
            ]
        },
        {
            "title": "## 💸 Tech Debt",
            "labels": [
                "tech-debt"
            ]
        },
        {
            "title": "## 🌐 Release Notes",
            "labels": [
                "customer",
                "customer-facing",
                "release",
                "release-notes"
            ]
        },
        {
            "title": "## 📚 Documentation",
            "labels": [
                "instructions",
                "documentation",
                "document",
                "docs",
                "readme",
                "playbook"
            ]
        },
        {
            "title": "## ⚓ Onboarding",
            "labels": [
                "onboarding",
                "onboarding-ops"
            ]
        },
        {
            "title": "## 🎲 Uncategorized",
            "labels": []
        }
    ],
    "ignore_labels": [
      "ignore",
      "dependencies"
    ],
    "sort": "ASC",
    "template": "#{{CHANGELOG}}",
    "pr_template": "- #{{TITLE}}\n   - PR: ##{{NUMBER}}",
    "empty_template": "- no changes",
    "label_extractor": [
        {
            "pattern": "(\\w+)",
            "on_property": "title",
            "method": "match"
        }
    ],
    "max_tags_to_fetch": 50,
    "max_pull_requests": 300,
    "max_back_track_time_days": 60,
    "tag_resolver": {
      "method": "sort",
      "filter": {
        "pattern": "v-(.+)",
        "flags": "gu"
      }
    },
    "base_branches": [
      "master"
    ]
}
