{
    "defaults": {
        "timeout": 120000,
        "viewport": {
            "width": 320,
            "height": 480
        },
        "chromeLaunchConfig": {
            "args": ["--no-sandbox", "--disable-setuid-sandbox"]
        },
        "reporters": [
            "cli",
            "pa11y-ci-reporter-cli-summary"
        ],
        "standard": "WCAG2AAA",
        "runners": ["htmlcs", "axe"]
    },
    "urls": [
        {
            "url": "https://reportstream.cdc.gov/*",
            "ignore": []
        }
    ]
}
