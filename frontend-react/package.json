  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZER=true yarn run build:production",
    "clean": "rimraf ./build &&  rimraf ./src/content/generated",
    "start:localdev": "cross-env NODE_ENV=development npm-run-all -p watch-scss start-js",
    "build:demo1": "cross-env NODE_ENV=demo1 yarn build-base-prod",
    "build:demo2": "cross-env NODE_ENV=demo2 yarn build-base-prod",
    "build:demo3": "cross-env NODE_ENV=demo3 yarn build-base-prod",
    "build:test": "cross-env NODE_ENV=test yarn build-base-prod",
    "build:staging": "cross-env NODE_ENV=staging yarn build-base-prod",
    "build:production": "cross-env NODE_ENV=production yarn build-base-prod",
    "build:localdev:csp": "cross-env NODE_ENV=dev.csp yarn build-base-dev",
    "build-base-prod": "yarn compile-scss-prod && node scripts/build.js && yarn copy-404page",
    "build-base-dev": "yarn compile-scss-dev && node scripts/build.js && yarn copy-404page",
    "start-js": "node scripts/start.js",
    "copy-404page": "cp build/index.html build/404.html",
    "compile-scss-prod": "sass --load-path=./node_modules/@uswds/uswds/packages --no-source-map --style=compressed --quiet src/global.scss:src/content/generated/global.out.css",
    "compile-scss-dev": "sass --load-path=./node_modules/@uswds/uswds/packages --embed-source-map --quiet-deps src/global.scss:src/content/generated/global.out.css",
    "cypress:local:run": "cross-env CYPRESS_ENV=development yarn cypress run",
    "cypress:local:open": "cross-env CYPRESS_ENV=development yarn cypress open",
    "cypress:staging:run": "echo 'not implemented yet'",
    "cypress:staging:open": "echo 'not implemented yet'",
    "cypress:production:run": "echo 'not implemented yet'",
    "cypress:production:open": "echo 'not implemented yet'",
    "watch-scss": "yarn compile-scss-dev && sass --load-path=./node_modules/@uswds/uswds/packages --embed-source-map --quiet-deps -w src/global.scss:src/content/generated/global.out.css",
    "test": "jest --watch",
    "test:debug": "jest --watch --runInBand",
    "test:ci": "jest --ci --runInBand --coverage --verbose --silent",
    "lint": "npm-run-all -p lint:eslint",
    "lint:write": "npm-run-all -p lint:eslint:write",
    "lint:eslint": "cross-env NODE_ENV=production eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "lint:eslint:write": "cross-env NODE_ENV=production eslint --fix \"src/**/*.{js,ts,jsx,tsx}\"",
    "yarn:show-outdated-packages": "yarn outdated",
    "run-build-dir": "yarn build:localdev:csp && yarn global add serve && serve -s build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
