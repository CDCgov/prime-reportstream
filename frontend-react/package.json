{
    "name": "react-frontend",
    "version": "0.1.1",
    "private": true,
    "npmClient": "yarn",
    "proxy": "http://localhost:7071",
    "dependencies": {
        "@craco/craco": "^6.4.3",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.16",
        "@okta/okta-auth-js": "^5.11.0",
        "@okta/okta-react": "^6.4.1",
        "@okta/okta-signin-widget": "^5.16.1",
        "@rest-hooks/hooks": "^2.0.0",
        "@rest-hooks/rest": "^2.2.0",
        "@rest-hooks/test": "^7.1.0",
        "@testing-library/user-event": "^13.5.0",
        "@trussworks/react-uswds": "^2.6.0",
        "@types/downloadjs": "^1.4.2",
        "@types/hookrouter": "^2.2.5",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^15.3.0",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.5",
        "craco-sass-resources-loader": "^1.1.0",
        "dompurify": "^2.3.4",
        "downloadjs": "^1.4.7",
        "env-cmd": "^10.1.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "font-awesome": "^4.7.0",
        "hookrouter": "^1.2.5",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "react": "^17.0.2",
        "react-chartjs-2": "^4.0.0",
        "react-device-detect": "^2.0.0",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-idle-timer": "^4.6.4",
        "react-router-dom": "^5.2.0",
        "react-toastify": "^8.1.0",
        "rest-hooks": "^6.1.7",
        "rimraf": "^3.0.2",
        "sass-loader": "^12.4.0",
        "spinners-react": "^1.0.6",
        "tslint": "^6.1.3",
        "typescript": "^4.5.4",
        "uswds": "^2.13.0",
        "web-vitals": "^2.1.2"
    },
    "scripts": {
        "clean": "rimraf ./build",
        "start:localdev": "env-cmd -f .env.development craco start",
        "build:staging": "env-cmd -f .env.staging craco build",
        "build:production": "env-cmd -f .env.production craco build",
        "postbuild:staging": "cp build/index.html build/404.html",
        "postbuild:production": "cp build/index.html build/404.html",
        "test": "craco test",
        "test:ci": "CI=true yarn test --coverage",
        "eject": "react-scripts eject",
        "lint": "npm-run-all -p lint:eslint lint:prettier",
        "lint:write": "npm-run-all -p lint:eslint:write lint:prettier:write",
        "lint:eslint": "eslint src/**/*.{js,ts,jsx,tsx}",
        "lint:eslint:write": "eslint --fix src/**/*.{js,ts,jsx,tsx}",
        "lint:prettier": "prettier --check src/**/*.{js,ts,jsx,tsx}",
        "lint:prettier:write": "prettier --write src/**/*.{js,ts,jsx,tsx}",
        "cypress-open": "./node_modules/.bin/cypress open",
        "cypress-run": "./node_modules/.bin/cypress run",
        "yarn:show-outdated-packages": "yarn outdated"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "prettier",
            "plugin:import/errors",
            "plugin:import/warnings"
        ],
        "env": {
            "browser": true,
            "node": false,
            "es6": true
        },
        "globals": {
            "RequestInit": true
        },
        "rules": {
            "import/no-unresolved": 0,
            "import/first": 1,
            "import/order": [
                1,
                {
                    "newlines-between": "always"
                }
            ],
            "import/newline-after-import": 1,
            "import/no-commonjs": 0
        },
        "settings": {
            "import/resolver": {}
        },
        "ignorePatterns": [
            "node_modules/",
            "build/"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@types/dompurify": "^2.3.1",
        "@types/jest": "^27.0.3",
        "@types/react-helmet": "^6.1.4",
        "@types/react-router-dom": "^5.3.2",
        "@types/testing-library__jest-dom": "^5.14.2",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "cypress": "^9.1.1",
        "eslint": "^7.26.0",
        "eslint-config-airbnb": "^19.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-import": "^2.23.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest-canvas-mock": "^2.3.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.0",
        "react-dropdown": "^1.9.2",
        "react-scripts": "^4.0.3",
        "sass": "^1.45.2",
        "stylelint": "^14.2.0",
        "yaml-loader": "^0.6.0"
    },
    "resolutions": {
        "react-scripts/node-forge": "^1.0.0",
        "react-scripts/postcss": "8.2.13",
        "react-scripts/resolve-url-loader/postcss": "8.2.13"
    }
}
