{
    "name": "react-frontend",
    "version": "0.1.0",
    "private": true,
    "npmClient": "yarn",
    "dependencies": {
        "@cdc/map": "^2.6.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.15",
        "@okta/okta-auth-js": "^5.2.3",
        "@okta/okta-react": "^6.1.0",
        "@okta/okta-signin-widget": "^5.9.0",
        "@rest-hooks/hooks": "^2.1.0",
        "@rest-hooks/rest": "^3.0.0",
        "@rest-hooks/test": "^7.1.0",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@trussworks/react-uswds": "^2.0.0",
        "@types/downloadjs": "^1.4.2",
        "@types/react-router-dom": "^5.3.0",
        "downloadjs": "^1.4.7",
        "hookrouter": "^1.2.5",
        "moment": "^2.29.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.3.0",
        "rest-hooks": "^6.1.0",
        "spinners-react": "^1.0.4",
        "uswds": "^2.12.1",
        "web-vitals": "^1.1.2",
        "zustand": "^3.5.8"
    },
    "devDependencies": {
        "@types/hookrouter": "^2.2.5",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-prettier": "^4.0.0",
        "hookrouter": "^1.2.5",
        "prettier": "^2.4.1",
        "react-scripts": "4.0.3",
        "rimraf": "^3.0.2",
        "sass": "^1.42.0",
        "tslint": "^6.1.3",
        "typescript": "^4.2.4"
    },
    "reactSnap": {
        "inlineCss": false
    },
    "scripts": {
        "clean": "rimraf ./build",
        "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
        "start-https": "HTTPS=true react-scripts start",
        "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
        "test": "react-scripts test",
        "eject-oneway": "react-scripts eject",
        "lint:eslint": "eslint --quiet 'src/**/*.{js,ts,jsx,tsx}'",
        "cypress-open": "./node_modules/.bin/cypress open",
        "cypress-run": "./node_modules/.bin/cypress run"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "eslint-config-airbnb-base"
        ],
        "env": {
            "browser": true,
            "node": false,
            "es6": true
        },
        "globals": {
            "RequestInit": true
        },
        "rules": {
            "import/extensions": "off",
            "import/prefer-default-export": "off",
            "no-multi-spaces": "off",
            "no-underscore-dangle": "off",
            "no-use-before-define": "off",
            "object-curly-newline": "off",
            "@typescript-eslint/no-use-before-define": ["warn"],
            "arrow-body-style": "warn",
            "arrow-parens": "warn",
            "block-spacing": "warn",
            "comma-dangle": "warn",
            "eol-last": "warn",
            "import/order": "warn",
            "indent": ["warn", 2],
            "key-spacing": "warn",
            "keyword-spacing": "warn",
            "max-len": ["warn", 102],
            "no-multiple-empty-lines": "warn",
            "no-nested-ternary": "warn",
            "no-trailing-spaces": "warn",
            "no-unused-vars": "warn",
            "object-curly-spacing": "warn",
            "object-shorthand": "warn",
            "operator-linebreak": "warn",
            "padded-blocks": "warn",
            "prefer-template": "warn",
            "quotes": "warn",
            "semi": "warn",
            "sort-imports": "warn",
            "space-before-function-paren": "warn"
        },
        "settings": {
            "import/resolver": {
                "node": {
                    "extensions": [
                        ".js",
                        ".jsx",
                        ".ts",
                        ".tsx"
                    ],
                    "moduleDirectory": [
                        "node_modules",
                        "src/"
                    ]
                }
            }
        },
        "ignorePatterns": [
            "node_modules/",
            "build/"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
