    "scripts": {
        "postinstall": "scripts/postinstall.sh",
        "clean": "rimraf ./build",
        "start:localdev": "cross-env NODE_ENV=development yarn run start-js",
        "preview": "cross-env NODE_ENV=preview yarn run build-base && yarn run vite preview",
        "build:demo1": "cross-env NODE_ENV=demo1 yarn build-base",
        "build:demo2": "cross-env NODE_ENV=demo2 yarn build-base",
        "build:demo3": "cross-env NODE_ENV=demo3 yarn build-base",
        "build:test": "cross-env NODE_ENV=test yarn build-base",
        "build:trialfrontend01": "cross-env NODE_ENV=trialfrontend01 yarn build-base",
        "build:trialfrontend02": "cross-env NODE_ENV=trialfrontend02 yarn build-base",
        "build:trialfrontend03": "cross-env NODE_ENV=trialfrontend03 yarn build-base",
        "build:staging": "cross-env NODE_ENV=staging yarn build-base",
        "build:production": "cross-env NODE_ENV=production yarn build-base",
        "build:production-sourcemaps": "cross-env NODE_ENV=production SOURCEMAPS=true yarn build-base",
        "build:localdev:csp": "cross-env NODE_ENV=dev.csp yarn build-base",
        "build-base": "yarn tsc --noEmit && yarn vite build",
        "start-js": "yarn run browserslist:generate && vite dev --port 3000",
        "cypress:local:run": "cross-env CYPRESS_ENV=development yarn cypress run",
        "cypress:local:open": "cross-env CYPRESS_ENV=development yarn cypress open",
        "cypress:staging:run": "echo 'not implemented yet'",
        "cypress:staging:open": "echo 'not implemented yet'",
        "cypress:production:run": "echo 'not implemented yet'",
        "cypress:production:open": "echo 'not implemented yet'",
        "test": "jest",
        "test:debug": "jest --watch --runInBand",
        "test:ci": "jest --ci --runInBand --coverage --verbose",
        "lint": "cross-env NODE_ENV=production eslint \"src/**/*.{js,ts,jsx,tsx}\"",
        "lint:write": "cross-env NODE_ENV=production eslint --fix \"src/**/*.{js,ts,jsx,tsx}\"",
        "yarn:show-outdated-packages": "yarn outdated",
        "run-build-dir": "yarn build:localdev:csp && yarn global add serve && serve -s build",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "browserslist:generate": "yarn dlx -p browserslist -p update-browserslist-db update-browserslist-db && ts-node-esm --project tsconfig.node.json scripts/generateBrowserslistRegex.ts"
    },
    "browserslist": {
        "production": [
            "last 2 chrome version",
            "last 2 and_chr version",
            "last 2 firefox version",
            "last 2 and_ff version",
            "last 2 safari version",
            "last 2 ios version",
            "last 2 edge version"
        ],
        "vite": [
            "chrome >= 87",
            "and_chr >= 87",
            "firefox >= 78",
            "and_ff >= 78",
            "safari >= 14",
            "ios >= 14",
            "edge >= 88"
        ]
    },
