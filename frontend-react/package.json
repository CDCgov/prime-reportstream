{
    "name": "react-frontend",
    "version": "0.1.0",
    "private": true,
    "npmClient": "yarn",
    "dependencies": {
        "@craco/craco": "^6.4.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.16",
        "@okta/okta-auth-js": "^5.2.3",
        "@okta/okta-react": "^6.1.0",
        "@okta/okta-signin-widget": "^5.9.0",
        "@rest-hooks/hooks": "^2.0.0",
        "@rest-hooks/rest": "^2.2.0",
        "@rest-hooks/test": "^7.1.0",
        "@trussworks/react-uswds": "^2.3.0",
        "@types/downloadjs": "^1.4.2",
        "@types/hookrouter": "^2.2.5",
        "@types/node": "^15.3.0",
        "@types/react": "^17.0.6",
        "@types/react-dom": "^17.0.5",
        "craco-sass-resources-loader": "^1.1.0",
        "dompurify": "^2.3.3",
        "downloadjs": "^1.4.7",
        "eslint-config-airbnb-base": "^14.2.1",
        "font-awesome": "^4.7.0",
        "hookrouter": "^1.2.5",
        "moment": "^2.29.1",
        "react": "^17.0.2",
        "react-chartjs-2": "^3.3.0",
        "react-device-detect": "^2.0.0",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-idle-timer": "^4.6.4",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "rest-hooks": "^5.3.4",
        "rimraf": "^3.0.2",
        "sass-loader": "^12.1.0",
        "spinners-react": "^1.0.6",
        "tslint": "^6.1.3",
        "typescript": "^4.2.4",
        "uswds": "^2.12.1",
        "web-vitals": "^1.1.2",
        "zustand": "^3.5.8"
    },
    "scripts": {
        "clean": "rimraf ./build",
        "start": "react-scripts start",
        "build": "craco build",
        "postbuild": "cp build/index.html build/404.html",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "npm-run-all -p lint:eslint lint:prettier",
        "lint:write": "npm-run-all -p lint:eslint:write lint:prettier:write",
        "lint:eslint": "eslint --quiet 'src/**/*.{js,ts,jsx,tsx}'",
        "lint:eslint:write": "eslint --fix 'src/**/*.{js,ts,jsx,tsx}'",
        "lint:prettier": "prettier -c 'src/**/*.{js,ts,jsx,tsx}'",
        "lint:prettier:write": "prettier --write 'src/**/*.{js,ts,jsx,tsx}'",
        "cypress-open": "./node_modules/.bin/cypress open",
        "cypress-run": "./node_modules/.bin/cypress run"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "prettier",
            "plugin:import/errors",
            "plugin:import/warnings"
        ],
        "env": {
            "browser": true,
            "node": false,
            "es6": true
        },
        "globals": {
            "RequestInit": true
        },
        "rules": {
            "import/no-unresolved": 0,
            "import/first": 1,
            "import/order": [
                1,
                {
                    "newlines-between": "always"
                }
            ],
            "import/newline-after-import": 1,
            "import/no-commonjs": 0
        },
        "settings": {
            "import/resolver": {}
        },
        "ignorePatterns": [
            "node_modules/",
            "build/"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^12.8.3",
        "@types/dompurify": "^2.3.1",
        "@types/jest": "^26.0.23",
        "@types/react-helmet": "^6.1.4",
        "@types/react-router-dom": "^5.3.2",
        "@types/testing-library__jest-dom": "^5.14.1",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "cypress": "^8.4.0",
        "eslint": "^7.26.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest-canvas-mock": "^2.3.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.4.1",
        "react-dropdown": "^1.9.2",
        "sass": "^1.42.1",
        "stylelint": "^14.0.0",
        "yaml-loader": "^0.6.0"
    }
}
