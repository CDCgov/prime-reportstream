  "jshintConfig": {
    "undef": true,
    "unused": false,
    "esversion": 8,
    "globals": {
      "window": true,
      "document": true,
      "axios": true,
      "bowser": true,
      "atob": true,
      "moment": true,
      "idleTimeout": true,
      "OktaSignIn": true,
      "download": true,
      "console": true,
      "Chart": true
